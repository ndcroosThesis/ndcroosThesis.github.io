<rdf:RDF
     xml:base="https://ndcroosthesis.github.io/example.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://ndcroosthesis.github.io/example.owl"/>
    
    <!-- See 3.1.1. Simple Named Classes https://www.w3.org/TR/2004/REC-owl-guide-20040210/#DefiningSimpleClasses -->
    <!-- Note that using rdf:about is equivalent to using rdf:ID -->
    
    <owl:Class rdf:about="https://ndcroosthesis.github.io/example.owl#sensorObservation"/>
    
    <owl:NamedIndividual rdf:about="https://ndcroosthesis.github.io/example.owl#observation1">
        <rdf:type rdf:resource="https://ndcroosthesis.github.io/example.owl#sensorObservation"/>
        <hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">5</hasValue>
    </owl:NamedIndividual>
    
    <owl:DatatypeProperty rdf:ID="https://ndcroosthesis.github.io/example.owl#hasValue"/>

</rdf:RDF>
